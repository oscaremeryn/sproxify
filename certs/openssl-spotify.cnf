[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = v3_req
x509_extensions    = v3_req

[ dn ]
CN = open.spotify.com

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = open.spotify.com
DNS.2 = open.spotify.com.

